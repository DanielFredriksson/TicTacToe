{
  "name": "tictactoe-challenge",
  "version": "1.0.0",
  "description": "TicTacToe Challenge Repository",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "start": "node dist/app.js",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/src/app.js",
    "test": "jest dist/test --forceExit",
    "prettier-write": "prettier --write \"src/**/*.ts\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/supertest": "^6.0.2",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": "18"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "globals": "^15.8.0",
    "husky": "^9.1.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  }
